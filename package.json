{
		"name": "sample_project",
		"version": "1.0.0",
		"description": "A simple Node API starter",
		"main": "app.js",
		"scripts": {
				"dev": "nodemon --exec babel-node ./src/app.js",
				"lint": "eslint src/  --fix",
				"clean": "rm -rf build && mkdir build",
				"build-babel": "babel ./src --out-dir ./build",
				"build": "npm run clean && npm run build-babel",
				"start": "npm run build && node ./build/server.js",
				"debug": "npm run dev",
				"test":"jest",
				"test1": "jest --colors --noStackTrace --passWithNoTests --runInBand",
				"test:unit": "npm test -- --watch -c jest-unit-config.js",
				"test:integration": "npm test -- --watch -c jest-integration-config.js",
				"test:staged": "npm test -- jest-unit-config.js",
				"test:ci": "npm test -- --coverage --silent",
				"test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
		},
		"standard": {
			"env": [
			  "jest"
			]
		  },
		"author": "",
		"license": "ISC",
		"dependencies": {
				"body-parser": "^1.19.0",
				"cors": "^2.8.5",
				"express": "^4.17.1",
				"helmet": "^3.22.0",
				"standard": "^14.3.3",
				"@babel/polyfill": "^7.2.5",
				"agenda": "^2.0.2",
				"awilix": "^4.2.2",
				"axios": "^0.19.0",
				"bcryptjs": "^2.4.3",
				"cloudinary": "^1.14.0",
				"dotenv": "^6.2.0",
				"fs-extra": "^8.1.0",
				"joi": "^14.3.0",
				"jsonwebtoken": "^8.5.1",
				"mongoose": "*",
				"morgan": "^1.9.1",
				"multer": "^1.4.2",
				"node-dependency-injection": "^2.3.5",
				"nodemailer": "^6.3.0",
				"nodemailer-mailgun-transport": "^1.4.0",
				"nodemailer-smtp-transport": "^2.7.4",
				"passport": "^0.4.0",
				"passport-jwt": "^4.0.0",
				"typedi": "^0.8.0",
				"uuid": "^3.3.3",
				"winston": "^3.2.1",
				"fast-glob": "^3.1.1",
				"validator": "^13.7.0"
		},
		"devDependencies": {
				"@babel/cli": "^7.2.3",
				"@babel/core": "^7.2.2",
				"@babel/node": "^7.2.2",
				"@babel/preset-env": "^7.2.2",
				"@babel/register": "^7.0.0",
				"babel-plugin-istanbul": "^5.1.0",
				"cross-env": "^5.2.0",
				"eslint": "^5.11.0",
				"eslint-config-airbnb": "^17.1.0",
				"eslint-plugin-import": "^2.14.0",
				"nodemon": "^1.18.9",
				"@shelf/jest-mongodb": "^1.1.3",
				"coveralls": "^3.0.11",
				"husky": "^4.0.0-beta.5",
				"jest": "^24.9.0",
				"lint-staged": "^9.4.3",
				"standard": "^14.3.1",
				"supertest": "^4.0.2"
		}
}
